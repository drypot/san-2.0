                                                                                                                        /*
--------------------------------------------------------------------------------
   control characters                                            ascii table
--------------------------------------------------------------------------------                                        */

#define AT_NUL                    0x00    //  000  null character
#define AT_SOH                    0x01    //  001  start of header
#define AT_STX                    0x02    //  002  start of text
#define AT_ETX                    0x03    //  003  end of text
#define AT_EOT                    0x04    //  004  end of transmission
#define AT_ENQ                    0x05    //  005  enquire
#define AT_ACK                    0x06    //  006  acknowledge
#define AT_BEL                    0x07    //  007  bell
#define AT_BS                     0x08    //  008  backspace
#define AT_HT                     0x09    //  009  horizontal tab
#define AT_LF                     0x0A    //  010  line feed
#define AT_VT                     0x0B    //  011  vertical tab
#define AT_FF                     0x0C    //  012  form feed
#define AT_CR                     0x0D    //  013  carriage return
#define AT_SO                     0x0E    //  014  shift out
#define AT_SI                     0x0F    //  015  shift in
#define AT_DLE                    0x10    //  016  data link escape
#define AT_DC1                    0x11    //  017  device control 1
#define AT_DC2                    0x12    //  018  device control 2
#define AT_DC3                    0x13    //  019  device control 3
#define AT_DC4                    0x14    //  020  device control 4
#define AT_NAK                    0x15    //  021  negative acknowledge
#define AT_SYN                    0x16    //  022  synchronize
#define AT_ETB                    0x17    //  023  end of text block
#define AT_CAN                    0x18    //  024  cancel
#define AT_EM                     0x19    //  025  end of medium
#define AT_SUB                    0x1A    //  026  substitute
#define AT_ESC                    0x1B    //  027  escape
#define AT_FS                     0x1C    //  028  file separator
#define AT_GS                     0x1D    //  029  group separator
#define AT_RS                     0x1E    //  030  record separator
#define AT_US                     0x1F    //  031  unit separator
#define AT_DEL                    0x7F    //  127  delete
                                                                                                                        /*
--------------------------------------------------------------------------------
   control characters concerned with keys                        ascii table
--------------------------------------------------------------------------------                                        */

#define AT_CTRL_A                 0x01    //  001  SOH
#define AT_CTRL_B                 0x02    //  002  STX
#define AT_CTRL_C                 0x03    //  003  ETX
#define AT_CTRL_D                 0x04    //  004  EOT
#define AT_CTRL_E                 0x05    //  005  ENQ
#define AT_CTRL_F                 0x06    //  006  ACK
#define AT_CTRL_G                 0x07    //  007  BEL
#define AT_CTRL_H                 0x08    //  008  BS
#define AT_CTRL_I                 0x09    //  009  HT
#define AT_CTRL_J                 0x0A    //  010  LF
#define AT_CTRL_K                 0x0B    //  011  VT
#define AT_CTRL_L                 0x0C    //  012  FF
#define AT_CTRL_M                 0x0D    //  013  CR
#define AT_CTRL_N                 0x0E    //  014  SO
#define AT_CTRL_O                 0x0F    //  015  SI
#define AT_CTRL_P                 0x10    //  016  DLE
#define AT_CTRL_Q                 0x11    //  017  DC1
#define AT_CTRL_R                 0x12    //  018  DC2
#define AT_CTRL_S                 0x13    //  019  DC3
#define AT_CTRL_T                 0x14    //  020  DC4
#define AT_CTRL_U                 0x15    //  021  NAK
#define AT_CTRL_V                 0x16    //  022  SYN
#define AT_CTRL_W                 0x17    //  023  ETB
#define AT_CTRL_X                 0x18    //  024  CAN
#define AT_CTRL_Y                 0x19    //  025  EM
#define AT_CTRL_Z                 0x1A    //  026  SUB
                                                                                                                        /*
--------------------------------------------------------------------------------
   digit characters                                              ascii table
--------------------------------------------------------------------------------                                        */

#define AT_0                      0x30    //  048                      [ 0 ]
#define AT_1                      0x31    //  049                      [ 1 ]
#define AT_2                      0x32    //  050                      [ 2 ]
#define AT_3                      0x33    //  051                      [ 3 ]
#define AT_4                      0x34    //  052                      [ 4 ]
#define AT_5                      0x35    //  053                      [ 5 ]
#define AT_6                      0x36    //  054                      [ 6 ]
#define AT_7                      0x37    //  055                      [ 7 ]
#define AT_8                      0x38    //  056                      [ 8 ]
#define AT_9                      0x39    //  057                      [ 9 ]
                                                                                                                        /*
--------------------------------------------------------------------------------
   control characters concerned with keys                        ascii table
--------------------------------------------------------------------------------                                        */

#define AT_A                      0x41    //  065                      [ A ]
#define AT_B                      0x42    //  066                      [ B ]
#define AT_C                      0x43    //  067                      [ C ]
#define AT_D                      0x44    //  068                      [ D ]
#define AT_E                      0x45    //  069                      [ E ]
#define AT_F                      0x46    //  070                      [ F ]
#define AT_G                      0x47    //  071                      [ G ]
#define AT_H                      0x48    //  072                      [ H ]
#define AT_I                      0x49    //  073                      [ I ]
#define AT_J                      0x4A    //  074                      [ J ]
#define AT_K                      0x4B    //  075                      [ K ]
#define AT_L                      0x4C    //  076                      [ L ]
#define AT_M                      0x4D    //  077                      [ M ]
#define AT_N                      0x4E    //  078                      [ N ]
#define AT_O                      0x4F    //  079                      [ O ]
#define AT_P                      0x50    //  080                      [ P ]
#define AT_Q                      0x51    //  081                      [ Q ]
#define AT_R                      0x52    //  082                      [ R ]
#define AT_S                      0x53    //  083                      [ S ]
#define AT_T                      0x54    //  084                      [ T ]
#define AT_U                      0x55    //  085                      [ U ]
#define AT_V                      0x56    //  086                      [ V ]
#define AT_W                      0x57    //  087                      [ W ]
#define AT_X                      0x58    //  088                      [ X ]
#define AT_Y                      0x59    //  089                      [ Y ]
#define AT_Z                      0x5A    //  090                      [ Z ]

#define AT_a                      0x61    //  097                      [ a ]
#define AT_b                      0x62    //  098                      [ b ]
#define AT_c                      0x63    //  099                      [ c ]
#define AT_d                      0x64    //  100                      [ d ]
#define AT_e                      0x65    //  101                      [ e ]
#define AT_f                      0x66    //  102                      [ f ]
#define AT_g                      0x67    //  103                      [ g ]
#define AT_h                      0x68    //  104                      [ h ]
#define AT_i                      0x69    //  105                      [ i ]
#define AT_j                      0x6A    //  106                      [ j ]
#define AT_k                      0x6B    //  107                      [ k ]
#define AT_l                      0x6C    //  108                      [ l ]
#define AT_m                      0x6D    //  109                      [ m ]
#define AT_n                      0x6E    //  110                      [ n ]
#define AT_o                      0x6F    //  111                      [ o ]
#define AT_p                      0x70    //  112                      [ p ]
#define AT_q                      0x71    //  113                      [ q ]
#define AT_r                      0x72    //  114                      [ r ]
#define AT_s                      0x73    //  115                      [ s ]
#define AT_t                      0x74    //  116                      [ t ]
#define AT_u                      0x75    //  117                      [ u ]
#define AT_v                      0x76    //  118                      [ v ]
#define AT_w                      0x77    //  119                      [ w ]
#define AT_x                      0x78    //  120                      [ x ]
#define AT_y                      0x79    //  121                      [ y ]
#define AT_z                      0x7A    //  122                      [ z ]
                                                                                                                        /*
--------------------------------------------------------------------------------
   punctuation characters                                        ascii table
--------------------------------------------------------------------------------                                        */

#define AT_SPACE                  0x20    //  032                      [   ]
#define AT_EXCLAMATION_POINT      0x21    //  033                      [ ! ]
#define AT_QUOTATION_MARK         0x22    //  034                      [ " ]
#define AT_NUMBER_SIGN            0x23    //  035                      [ # ]
#define AT_DOLLOR_SIGN            0x24    //  036                      [ $ ]
#define AT_PERCENT_SIGN           0x25    //  037                      [ % ]
#define AT_AMPERSAND              0x26    //  038                      [ & ]
#define AT_APOSTROPHE             0x27    //  039                      [ ' ]
#define AT_OPENING_PARENTHESIS    0x28    //  040                      [ ( ]
#define AT_CLOSING_PARENTHESIS    0x29    //  041                      [ ) ]
#define AT_ASTERISK               0x2A    //  042                      [ * ]
#define AT_PLUS_SIGN              0x2B    //  043                      [ + ]
#define AT_COMMA                  0x2C    //  044                      [ , ]
#define AT_MINUS_SIGN             0x2D    //  045 ***                  [ - ]
#define AT_HYPHEN                 0x2D    //  045                      [ - ]
#define AT_PERIOD                 0x2E    //  046                      [ . ]
#define AT_SLASH                  0x2F    //  047                      [ / ]

                                          //  digit characters gap

#define AT_COLON                  0x3A    //  058                      [ : ]
#define AT_SEMICOLON              0x3B    //  059                      [ ; ]
#define AT_LESS_THAN_SIGH         0x3C    //  060                      [ < ]
#define AT_EQUAL_SIGN             0x3D    //  061                      [ = ]
#define AT_GREATER_THAN_SIGN      0x3E    //  062                      [ > ]
#define AT_QUESTION_MARK          0x3F    //  063                      [ ? ]
#define AT_AT_SIGN                0x40    //  064                      [ @ ]

                                          //  upper characters gap

#define AT_OPENING_BRACKET        0x5B    //  091                      [ [ ]
#define AT_BACKWARD_SLASH         0x5C    //  092                      [ \ ]
#define AT_CLOSING_BRACKET        0x5D    //  093                      [ ] ]
#define AT_CARET                  0x5E    //  094                      [ ^ ]
#define AT_CIRCUM                 0x5E    //  094 ***                  [ ^ ]
#define AT_UNDERSCORE             0x5F    //  095                      [ _ ]
#define AT_GRAVE                  0x60    //  096                      [ ` ]

                                          //  lower characters gap

#define AT_OPENING_BRACE          0x7B    //  123                      [ { ]
#define AT_VERTICAL_LINE          0x7C    //  124                      [ | ]
#define AT_CLOSING_BRACE          0x7D    //  125                      [ } ]
#define AT_TILDE                  0x7E    //  126                      [ ~ ]
