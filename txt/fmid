--------------------------------------------------------------------------------
   Á¦¾î¹®ÀÚ                                                2 ¹ÙÀÌÆ® ³ª´®ÄÚµå
--------------------------------------------------------------------------------                                        */

#define NL_NUL                    0x0000  //  Null
#define NL_SOH                    0x0001  //  Start of header
#define NL_STX                    0x0002  //  Start of text
#define NL_ETX                    0x0003  //  End of text
#define NL_EOT                    0x0004  //  End of transmission
#define NL_ENQ                    0x0005  //  Enquiry
#define NL_ACK                    0x0006  //  Acknowledgement
#define NL_BEL                    0x0007  //  Bell
#define NL_BS                     0x0008  //  Backspace
#define NL_HT                     0x0009  //  Horizontal tab
#define NL_LF                     0x000A  //  Line feed
#define NL_VT                     0x000B  //  Vertical tab
#define NL_FF                     0x000C  //  Form feed
#define NL_CR                     0x000D  //  Carriage return
#define NL_SO                     0x000E  //  Shift out
#define NL_SI                     0x000F  //  Shift in
#define NL_DLE                    0x0010  //  Data link escape
#define NL_DC1                    0x0011  //  Device control 1
#define NL_DC2                    0x0012  //  Device control 2
#define NL_DC3                    0x0013  //  Device control 3
#define NL_DC4                    0x0014  //  Device control 4
#define NL_NAK                    0x0015  //  Negative acknowledgment
#define NL_SYN                    0x0016  //  Synchronous idle
#define NL_ETB                    0x0017  //  End of transmission block
#define NL_CAN                    0x0018  //  Cancel
#define NL_EM                     0x0019  //  End of medium
#define NL_SUB                    0x001A  //  Substitute
#define NL_ESC                    0x001B  //  Escape
#define NL_FS                     0x001C  //  File separator
#define NL_GS                     0x001D  //  Group separator
#define NL_RS                     0x001E  //  Record separator
#define NL_US                     0x001F  //  Unit separator
