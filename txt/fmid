                                                                                                                        /*
--------------------------------------------------------------------------------
   control characters                                            ascii table
--------------------------------------------------------------------------------                                        */

#define AT_NUL                    0x00    //  000  null character
#define AT_SOH                    0x01    //  001  start of header
#define AT_STX                    0x02    //  002  start of text
#define AT_ETX                    0x03    //  003  end of text
#define AT_EOT                    0x04    //  004  end of transmission
#define AT_ENQ                    0x05    //  005  enquire
#define AT_ACK                    0x06    //  006  acknowledge
#define AT_BEL                    0x07    //  007  bell
#define AT_BS                     0x08    //  008  backspace
#define AT_HT                     0x09    //  009  horizontal tab
#define AT_LF                     0x0A    //  010  line feed
#define AT_VT                     0x0B    //  011  vertical tab
#define AT_FF                     0x0C    //  012  form feed
#define AT_CR                     0x0D    //  013  carriage return
#define AT_SO                     0x0E    //  014  shift out
#define AT_SI                     0x0F    //  015  shift in
#define AT_DLE                    0x10    //  016  data link escape
#define AT_DC1                    0x11    //  017  device control 1
#define AT_DC2                    0x12    //  018  device control 2
#define AT_DC3                    0x13    //  019  device control 3
#define AT_DC4                    0x14    //  020  device control 4
#define AT_NAK                    0x15    //  021  negative acknowledge
#define AT_SYN                    0x16    //  022  synchronize
#define AT_ETB                    0x17    //  023  end of text block
#define AT_CAN                    0x18    //  024  cancel
#define AT_EM                     0x19    //  025  end of medium
#define AT_SUB                    0x1A    //  026  substitute
#define AT_ESC                    0x1B    //  027  escape
#define AT_FS                     0x1C    //  028  file separator
#define AT_GS                     0x1D    //  029  group separator
#define AT_RS                     0x1E    //  030  record separator
#define AT_US                     0x1F    //  031  unit separator
#define AT_DEL                    0x7F    //  127  delete
                                                                                                                        /*
--------------------------------------------------------------------------------
   control characters concerned with keys                        ascii table
--------------------------------------------------------------------------------                                        */

#define AT_CTRL_A                 0x01    //  001  SOH
#define AT_CTRL_B                 0x02    //  002  STX
#define AT_CTRL_C                 0x03    //  003  ETX
#define AT_CTRL_D                 0x04    //  004  EOT
#define AT_CTRL_E                 0x05    //  005  ENQ
#define AT_CTRL_F                 0x06    //  006  ACK
#define AT_CTRL_G                 0x07    //  007  BEL
#define AT_CTRL_H                 0x08    //  008  BS
#define AT_CTRL_I                 0x09    //  009  HT
#define AT_CTRL_J                 0x0A    //  010  LF
#define AT_CTRL_K                 0x0B    //  011  VT
#define AT_CTRL_L                 0x0C    //  012  FF
#define AT_CTRL_M                 0x0D    //  013  CR
#define AT_CTRL_N                 0x0E    //  014  SO
#define AT_CTRL_O                 0x0F    //  015  SI
#define AT_CTRL_P                 0x10    //  016  DLE
#define AT_CTRL_Q                 0x11    //  017  DC1
#define AT_CTRL_R                 0x12    //  018  DC2
#define AT_CTRL_S                 0x13    //  019  DC3
#define AT_CTRL_T                 0x14    //  020  DC4
#define AT_CTRL_U                 0x15    //  021  NAK
#define AT_CTRL_V                 0x16    //  022  SYN
#define AT_CTRL_W                 0x17    //  023  ETB
#define AT_CTRL_X                 0x18    //  024  CAN
#define AT_CTRL_Y                 0x19    //  025  EM
#define AT_CTRL_Z                 0x1A    //  026  SUB
                                                                                                                        /*
--------------------------------------------------------------------------------
   digit characters                                              ascii table
--------------------------------------------------------------------------------                                        */

#define AT_0                      0x30    //  048                      [ 0 ]
#define AT_1                      0x31    //  049                      [ 1 ]
#define AT_2                      0x32    //  050                      [ 2 ]
#define AT_3                      0x33    //  051                      [ 3 ]
#define AT_4                      0x34    //  052                      [ 4 ]
#define AT_5                      0x35    //  053                      [ 5 ]
#define AT_6                      0x36    //  054                      [ 6 ]
#define AT_7                      0x37    //  055                      [ 7 ]
#define AT_8                      0x38    //  056                      [ 8 ]
#define AT_9                      0x39    //  057                      [ 9 ]
                                                                                                                        /*
--------------------------------------------------------------------------------
   control characters concerned with keys                        ascii table
