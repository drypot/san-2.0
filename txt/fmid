#define AT_S                      0x53    //  083                      [ S ]
#define AT_T                      0x54    //  084                      [ T ]
#define AT_U                      0x55    //  085                      [ U ]
#define AT_V                      0x56    //  086                      [ V ]
#define AT_W                      0x57    //  087                      [ W ]
#define AT_X                      0x58    //  088                      [ X ]
#define AT_Y                      0x59    //  089                      [ Y ]
#define AT_Z                      0x5A    //  090                      [ Z ]

#define AT_a                      0x61    //  097                      [ a ]
#define AT_b                      0x62    //  098                      [ b ]
#define AT_c                      0x63    //  099                      [ c ]
#define AT_d                      0x64    //  100                      [ d ]
#define AT_e                      0x65    //  101                      [ e ]
#define AT_f                      0x66    //  102                      [ f ]
#define AT_g                      0x67    //  103                      [ g ]
#define AT_h                      0x68    //  104                      [ h ]
#define AT_i                      0x69    //  105                      [ i ]
#define AT_j                      0x6A    //  106                      [ j ]
#define AT_k                      0x6B    //  107                      [ k ]
#define AT_l                      0x6C    //  108                      [ l ]
#define AT_m                      0x6D    //  109                      [ m ]
#define AT_n                      0x6E    //  110                      [ n ]
#define AT_o                      0x6F    //  111                      [ o ]
#define AT_p                      0x70    //  112                      [ p ]
#define AT_q                      0x71    //  113                      [ q ]
#define AT_r                      0x72    //  114                      [ r ]
#define AT_s                      0x73    //  115                      [ s ]
#define AT_t                      0x74    //  116                      [ t ]
#define AT_u                      0x75    //  117                      [ u ]
#define AT_v                      0x76    //  118                      [ v ]
#define AT_w                      0x77    //  119                      [ w ]
#define AT_x                      0x78    //  120                      [ x ]
#define AT_y                      0x79    //  121                      [ y ]
#define AT_z                      0x7A    //  122                      [ z ]
                                                                                                                        /*
--------------------------------------------------------------------------------
   punctuation characters                                        ascii table
--------------------------------------------------------------------------------                                        */

#define AT_SPACE                  0x20    //  032                      [   ]
#define AT_EXCLAMATION_POINT      0x21    //  033                      [ ! ]
#define AT_QUOTATION_MARK         0x22    //  034                      [ " ]
#define AT_NUMBER_SIGN            0x23    //  035                      [ # ]
#define AT_DOLLOR_SIGN            0x24    //  036                      [ $ ]
#define AT_PERCENT_SIGN           0x25    //  037                      [ % ]
#define AT_AMPERSAND              0x26    //  038                      [ & ]
#define AT_APOSTROPHE             0x27    //  039                      [ ' ]
#define AT_OPENING_PARENTHESIS    0x28    //  040                      [ ( ]
#define AT_CLOSING_PARENTHESIS    0x29    //  041                      [ ) ]
#define AT_ASTERISK               0x2A    //  042                      [ * ]
#define AT_PLUS_SIGN              0x2B    //  043                      [ + ]
#define AT_COMMA                  0x2C    //  044                      [ , ]
#define AT_MINUS_SIGN             0x2D    //  045 ***                  [ - ]
#define AT_HYPHEN                 0x2D    //  045                      [ - ]
#define AT_PERIOD                 0x2E    //  046                      [ . ]
#define AT_SLASH                  0x2F    //  047                      [ / ]

                                          //  digit characters gap

#define AT_COLON                  0x3A    //  058                      [ : ]
#define AT_SEMICOLON              0x3B    //  059                      [ ; ]
#define AT_LESS_THAN_SIGH         0x3C    //  060                      [ < ]
#define AT_EQUAL_SIGN             0x3D    //  061                      [ = ]
#define AT_GREATER_THAN_SIGN      0x3E    //  062                      [ > ]
#define AT_QUESTION_MARK          0x3F    //  063                      [ ? ]
#define AT_AT_SIGN                0x40    //  064                      [ @ ]

                                          //  upper characters gap

#define AT_OPENING_BRACKET        0x5B    //  091                      [ [ ]
#define AT_BACKWARD_SLASH         0x5C    //  092                      [ \ ]
#define AT_CLOSING_BRACKET        0x5D    //  093                      [ ] ]
#define AT_CARET                  0x5E    //  094                      [ ^ ]
#define AT_CIRCUM                 0x5E    //  094 ***                  [ ^ ]
#define AT_UNDERSCORE             0x5F    //  095                      [ _ ]
#define AT_GRAVE                  0x60    //  096                      [ ` ]

                                          //  lower characters gap

#define AT_OPENING_BRACE          0x7B    //  123                      [ { ]
#define AT_VERTICAL_LINE          0x7C    //  124                      [ | ]
#define AT_CLOSING_BRACE          0x7D    //  125                      [ } ]
#define AT_TILDE                  0x7E    //  126                      [ ~ ]
